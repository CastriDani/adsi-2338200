Proceso Ejercico13
	//Pedir el radio de un círculo y calcular su área. A=PI*r^2.
	
	//Variables
	definir circle_radius Como Real;
	definir circle_area Como Real;
	
	//Entradas
	Escribir "Write circle radius : ";
	leer circle_radius;
	
	//Operaciones
	circle_area = 3.141592 * (circle_radius * circle_radius);
	
	//Salidas
	escribir "The area circle is : " , circle_area;
	
	
FinProceso
