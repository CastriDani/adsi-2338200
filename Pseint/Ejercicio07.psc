	//Hacer un algoritmo para sumar dos números, los cuales serán tecleados por el usuario. Mostrar el
	//resultado.
	
	
	//Variables
	Definir  num_1 Como Entero;
	Definir num_2 Como Entero;
	definir total Como Entero;
	
	//entrada
	Escribir  "Write a random number : ";
	leer num_1;
	Escribir  "Write a random number : ";
	leer num_2;
	
	//Operacion
	total = num_1 + num_2;
	
	//salida 
	Escribir "The total of the sum is : " , total;
	
