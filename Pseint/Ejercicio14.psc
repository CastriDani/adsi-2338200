Proceso Ejercicio14
	//Pedir el radio de una circunferencia y calcular su longitud.
	
	//Variables
	Definir Radius Como Real;
	definir Lenght Como Real;
	
	//Entradas
	Escribir "Write to Radius the circumference : ";
	Leer Radius;
	
	//Operaciones
	Lenght = 2 * pi * Radius;
	
	//Salidas
	Escribir "The length of the circumference is : " , Lenght;
FinProceso
